/- WordSystem

model WordSystem {
    me string: asGiven

    me bool: isCompatibleWith(our WordSystem: wordS) <- {
        if (self==NULL or wordS==NULL){return(true)}
        if(asGiven == "" or wordS.asGiven == ""){return(true)}
        if(asGiven == wordS.asGiven){return(true)}
        return(false)
    }
    void: clear() <- {asGiven<-""}
}
