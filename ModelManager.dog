struct ModelManager {
    our infon[multimap string]: classToModel
    void: indexDefinition(our WordDefn: classID, our infon: infModel) <- {
        classToModel.insert(classID.asGiven, infModel)
    }
    our infon: fetchModel(our WordDefn: classID) <- {
        our infon: retVal
        me string: asGiven <- classID.asGiven
        withEach wordInfon in classToModel from asGiven to asGiven{
            retVal <- wordInfon
        }
        return(retVal)
    }
}
